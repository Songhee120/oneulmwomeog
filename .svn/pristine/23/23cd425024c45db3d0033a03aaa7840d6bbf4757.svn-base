}

#dessert_area .dessertSelect_area {
	text-align: center;
	padding: 2px;
}

#dessert_area .dessertSelect_area select {
	width: 50%;
	text-align-last: center;
}

#dessert_area .dessertNon_member {
	text-align: center;
	padding: 6px 0px;
}

#dessert_area .dessertNon_member input {
	width: 90%;
	border: none;
	background-color: #f4f4f4;
	text-align: center;
	font-size: 16px;
	height: 52px;
}

#dessert_area .dessertMap_api {
	border: 1px solid;
	border-radius: 14px;
	object-fit: contain;
	width: 95%;
	height: 70%;
}

#dessert_area .dessertMap_api img {
	border-radius: 16px;
	width: 100%;
	height: 100%;
}

#dessert_area .dessert_listRight {
	width: 40%;
	float: right;
	height: 517px;
}

#dessert_area .dessert_list {
	border: 1px solid;
	border-radius: 14px;
	object-fit: contain;
	width: 98%;
	height: 475px;
	margin: 10px 6px;
}

#dessert_area .dessert_list img {
	border-radius: 16px;
	width: 100%;
	height: 100%;
}

#dessert_area .desset_submit {
	height: 54px;
	position: relative;
	top: 82%;
}

#dessert_area .desset_submit button {
	width: 24%;
	position: absolute;
	height: 100%;
	border-radius: 23px;
	background-color: pink;
	left: 39%;
}

#findDessert_road {
	display: none;
}

#findDessert_road .dessertRoad_area {
	object-fit: contain;
	border: 1px solid;
}

#findDessert_road .dessertRoad_area img {
	width: 100%;
}
/* 후식뭐먹 끝! */

/*이용약관 css*/
#message{
	resize:none;
	border:1px solid;
	overflow-x:hidden;
	line-height:1em;
}
.agree_box{
	width:25px;height:25px;
	margin-left:0px;
}
#span_box{
	display:block;
	text-align:top;
}
#label_box{
	display:inline;
	vertical-align:top;
}
#need_span{
	font-size:15px;
	vertical-align:bottom;
	color:red;
}
#first_box_errmsg,
#second_box_errmsg{
	font-size:15px;
	display:inline;
	color:red;
} 
/* 회원가입, 로그인, 아이디, 비밀번호 찾기에 들어가는 label css */
#jogin_label{
   margin-bottom:0px;
   font-size:20px;
   font-weight:bold;
}
/* 회원가입 로그인에서의 span들의 class명 */
.jogin_span{
   font-size:14px;
   font-weight:bold;
   color: red;
   margin-bottom:0px;
}

/* input[text] */
.col-12 {
   margin: 0 auto;
   width: 50%;
}
