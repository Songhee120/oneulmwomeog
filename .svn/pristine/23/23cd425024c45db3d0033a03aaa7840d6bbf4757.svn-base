	#notice{
		font-size:13px;
	}
	#notice_select{
		text-align:left;
	}
	.select_type{
		width:80px;
		font-size:16px;
	}
	#notices_search{
		width:90px;
		position:relative;
		bottom:1px;
	}
	#notices_a{
		font-size:30px;
	}
