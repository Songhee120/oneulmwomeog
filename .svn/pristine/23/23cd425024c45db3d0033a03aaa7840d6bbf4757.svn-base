@charset "UTF-8";

/* 전체 버튼 적용 */
button{
	font-family:'Do Hyeon', sans-serif;
	border:1px solid #bdbdbd;
	background:white;
	color:black;
}
button:hover{
	background:#fbe9e7;
}
input[type="submit"]:hover{
	background:#fbe9e7;
}
#change_pw_btn{
	padding:0px;
	width:120px;
	height:60px;
	border-radius:10px;
}
.cmp_btn{
	width:120px;
	height:60px;
	border-radius:10px;
	font-size:15px;
}
.cmp_btn:hover{
	background:#fbe9e7;
}
#cmp_btn{
	background:white;
	color:black;
	border:1px solid #bdbdbd;
}
/* home부분  */
#my_home {
	height: 500px;
	margin-bottom: 0px;
}

#home_header {
	height: 150px;
	margin-bottom: 0px;
}

#home_header_div1 {
	width: 35%;
	height: 150px;
	display: inline-block;
	text-align: right;
}

#home_header_div2 {
	width: 65%;
	height: 150px;
	line-height: 163px;
	float: right;
}

#my_home_div1 {
	height: 100px;
	text-align: center;
}

#my_home_div2 {
	height: 250px;
	text-align: center;
	line-height: 250px;
}

#home_img {
	width: 130px;
	height: 130px;
	position: relative;
	top: 10px;
}

#home_name {
	font-size: 90px;
	color: #fbe9e7;
}

#home_select_btn {
	width: 200px;
	height: 100px;
	font-size: 25px;
}
.message{
	white-space:pre-line;
	border:1px solid;
}
#login_btn{
	width:120px;
	height:50px;
	background:white;
}
#gojoin_btn{
	width:120px;
	height:50px;
	background:white;
}
#login_btn:hover{
	background:#fbe9e7;
}
#gojoin_btn:hover{
	background:#fbe9e7;
}
#join_btn{
	border-radius:10px;
	background:white;
	color:black;
	border:2px solid #bdbdbd;
	height:50px;
	width:120px;
	padding:0px;
	font-size:18px;
	color:#616161;
}
#join_btn:hover{
	background:#fbe9e7;
}
/* 음식 뭐먹 부분 */
#food_area {
	display: none;
	height: 625px;
	margin-bottom: 0em;
}

#food_area .random_area {
	font-size: 34px; 
	text-align: center;
	padding: 10px;
}

#food_area .random_area p {
	margin-bottom: 0px;
}

#food_area .selectNmapLeft {
	width: 60%;
	float: left;
	height: 517px;
}

#food_area .select_nonMember {
	border: 1px solid;
	border-radius: 10px;
	height: 20%;
	width: 95%;
	margin: 10px 0px;	
}

#food_area .select_area {
	text-align: center;
	padding: 2px;
}

#food_area .select_area select {
	appearance: auto;
}

#food_area .non_member {
	text-align: center; 
	padding: 6px 0px;
}

#food_area .non_member input {
	width: 90%; 
	border: none; 
	background-color: #f4f4f4; 
	text-align: center; 
	font-size: 16px; 
	height: 52px;
}

#food_area .map_api {
	border: 1px solid; 
	border-radius: 14px; 
	object-fit : contain;
	width: 95%;
	height: 70%;
}

#food_area .map_api img {
	border-radius: 16px;
	width: 100%;
	height: 100%;
}

#food_area .food_listRight {
	width: 40%;
	float: right;
	height: 517px;
}

#food_area .food_list {
	border: 1px solid;
	border-radius: 14px;
	object-fit: contain;
	width: 98%;
	height: 475px;
	margin: 10px 6px;
}

#food_area .food_list img {
	border-radius: 16px;
	width: 100%;
	height: 100%;
}

#food_area .food_submit {
	height: 54px;
	position: relative;
	top: 82%;
}

#food_area .food_submit button {
	width: 24%; 
	position: absolute; 
	height: 100%; 
	border-radius: 23px; 
	background-color: pink;
	left: 39%;
}

#find_road {
	display: none;
}

#find_road .road_area{
	object-fit: contain;
	border: 1px solid;
}

#find_road .road_area img {
	width: 100%;
}
/* 음식 뭐먹 부분 끝 */

/* 후식뭐먹 시작 */
#dessert_area {
	display: none;
	height: 625px;
	margin-bottom: 0em;
}

#dessert_area .hello_dessert {
	font-size: 34px;
	text-align: center;
