}
/*공지사항 */
a {
	text-decoration:none;
}
textarea{
	resize:none;
}
#notices_a{
	font-size:45px;
	color:#ffcccc;
	font-weight:900;
}
.notice_div{
	padding:0px;
	height:auto;
}
#notice{
	width:100%;
	font-size:17px;
	margin:0
}
#notice_head{
	color:black;
}

#th, td{
	border:1px solid black;
	border-left:none;
	border-right:none;
	padding:5px;
	vertical-align:middle;
}
#notice tbody{
	text-align:center;	
}
tr:hover{
	background:#eeeeee;
}
#notice_title{
	text-align:left;
}
#notice_title a{
	text-decoration:none;
	color:#777777;
}
#notice_title a:hover{
	color:black;
}
#notice_select{
	text-align:left;
	width:100%;
}
.select_type{
	width:100px;
}
#notices_search{
