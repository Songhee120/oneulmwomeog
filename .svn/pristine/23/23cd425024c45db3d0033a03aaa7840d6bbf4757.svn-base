	display:inline-block;
	font-size:18px;
	color:black;
	height:26px;
	-webkit-appearance:auto;
	border-radius:5px;
}

#search_btn{
	position:relative;
	bottom:2px;
	width:50px;
	border-radius:5px;
	height:26px;
	padding:0px;
	background:white;
	color:black;
	border:1px solid #bdbdbd;
	font-size:13px;
}
#search_btn:hover{
	background:#fbe9e7;
}

select {
   appearance: auto;
}
.page_move{
	text-decoration:none;
	font-size:1.0em;
	color:black;
}
.page_move:hover{
	text-decoration:underline;
}
.profile {
    width: 60px;
    height: 60px; 
    border-radius: 70%;
    overflow: hidden;
    background-color:pink;
    display:block;
    float:left;
}
/* 리뷰페이지 */
/* 평점 별 */
.star_rating {
   font-size:0; 
   letter-spacing:-4px;
}

/* 평점 별 */
.star_rating a {
    font-size:45px;
    letter-spacing:0;
    display:inline-block;
    margin-left:5px;
    text-decoration:none;
    float:right;
    color:#f2f2f2;
   }
   
.star_rating a:first-child {margin-left:0;}

/* 평점 별 눌렀을 때 색깔 변경 */
.star_rating a.on {color:pink;}
/* 마이페이지 */
#my_inform_btn{
	font-size:5px;
	width:60px;
	height:30px;
	border-radius:10px;
	background:white;
	border:1px solid #777777;
	color:black;
}
#my_inform_btn:hover{
	background:#777777;
}

/* 리뷰 작성 */
#my_review{
	height:750px;
}
#my_review_div1{
	height:100px;
}
#my_review_div2{
	height:300px;
	border:1px solid black;
	border-radius:10px;
}
#my_review_div2-1{
	height:50px;
}
#star_grade{
	text-align:right;
	margin:0px;
}
#star_grade a{
	text-decoration:none;
	color:gray;
}
#star_grade a.on{
	color:red;
}
#my_review_div2-2{
	height:250px;
}
#text_limit{
	color:black;
}
#my_review_div3{
	height:30px;
	text-align:right;
}
#my_review_div4{
	height:190px;
}
/*12/07~12/08*/

.input_wrap label {
	display: inline-block;
	padding: .5em .75em;
	color: #999;
	font-size: inherit;
	line-height: normal;
	vertical-align: middle;
	background-color: #fdfdfd;
	cursor: pointer;
	border: 1px solid #ebebeb;
	border-bottom-color: #e2e2e2;
	border-radius: 10px;
	position:relative;
	bottom:23px;
}
#review_img {
	width: 100px;
	height: 100px;
	margin-left: 10px;
	border: 1px solid black;
	position: relative;
	top: 13%;
}
.imgs_wrap{
	border:1px solid;
	height:140px;
	margin-top:10px;
	border-radius:10px;
	overflow-x:scroll;
	white-space:nowrap;
	line-height:140px;
}
/*------------  */
#input_imgs:hover{
	background:#eeeeee;
}
#my_review_div5{
	height:100px;
	line-height:100px;
	margin-top:30px;
}
#review_stock_div{
	width:30%;
	height:100%;
	display:inline-block;
}
#review_work_div{
	width:70%;
	height:100%;
	float:right;
	text-align:right;
}
#review_btn{
	width:150px;
	height:70px;
	border-radius:10px;
	font-size:20px;
	font-weight:bold;
	text-align:center;
	color:black;
	border:1px solid #eeeeee;
	cursor:pointer;
	background:white;
}
#review_btn:hover{
	background:#fbe9e7;
}
#my_review_res{
	color:#e39964;
	position:relative;
	top:28%;
	font-size:45px;
}
#res_text{
	font-size:30px;
	color:black;
}
#my_review_text{
	width:100%;
	height:247px;
	border:none;
	padding:15px;
	outline:none;
	color:black;
	background:white;
}

/* home 반응형 */
@media(max-width:1660px){
	#main_logo{
		width:150px;
	}
	#logo_a{
		margin-left:-1%;
	}
}
@media(max-width:1280px){
	#mon, #day{
		margin-left:3.48%;
	}
}
@media(max-width:1024px){
	#mon, #day{
		margin-left:3.44%;
	}
}
@media(max-width:980px){
	#mon, #day{
		margin-left:2.8%;
	}
}
@media(max-width:736px){
	#home_header_div1 {
		height: 80px;
		width: 100%;
		text-align: center;
	}
	#home_div1-1 {
		line-height: 50px;
	}
	#home_img {
		width: 80px;
		height: 80px;
		top: 0px;
	}
	#home_header_div2 {
		height: 70px;
		width: 100%;
		text-align: center;
		line-height: 70px;
	}
	#home_name {
		font-size: 70px;
	}
	#my_home_div2 {
		line-height: 0px;
	}
	#home_select_btn {
		width: 150px;
		height: 70px;
		font-size: 21px;
		margin-top: 20px;
	}
	/* 아이디, 회원가입, 비밀번호 찾기 등 input(text)반응형  */
	.col-12{
		width:70%;
	}
	.find_id_pw{
		font-size:14px;
	}
	#jogin_label{
		font-size:14px;
	}
	#mon, #day{
		margin-left:2.83%;
	}
	/* 로그인 */
	#login_btn #gojoin_btn{
		width:50%;
		height:45px;
		margin-top:5px;
	}
	/*비밀번호, 아이디 찾기  */
	#find_btn{
		width:50%;
	}
	/*마이페이지 반응형 */
	#user_img{
		width:35px;
		height:35px;
	}
	#user_name{
		font-size:20px;
		top:12px;
		left:5px;
	}
	#user_nickname{
		bottom:15px;
		left:5px;
		font-size:15px;
	}
	#my_inform_btn{
		width:50px;
		height:25px;
		position:relative;
		top:5px;
		left:2px;
	}
	#logout_btn{
		height:25px;
		width:44px;
		font-size:2px;
	}
	#my_zzim{
		font-size:12px;
	}
	#mypage_nav{
		height:40%;
	}
	/* 리뷰 쓰기 반응형 */
	#my_review_res{
		font-size:30px;
	}
	#res_text{
		font-size:15px;
	}
	#my_review_div2{
		height:202px;
	}
	#my_review_div2-2,
	#my_review_text{
		height:150px;	
	}
	#my_review_text{
		border-radius:10px;
	}
	.input_wrap label{
		bottom:0px;
	}
	#review_img{
		width:70px;
		height:70px;
	}
	.imgs_wrap{
		line-height:165px;
	}
	#review_stock_div,
	#review_work_div{
		width:100%;
		text-align:center;
	}
	#review_stock_div{
		display:block;
	}
	#review_work_div{
		float:none;
	}
	#review_btn{
		width:110px;
		height:50px;
		font-size:15px;
	}
}
/* 공지사항 반응형 */
