<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http//mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="confirm">

<select id="getConfirmList" parameterType="map" resultType="confirmVO">
		SELECT * FROM (SELECT ROWNUM R, N.* FROM (SELECT * FROM TBL_CONFIRM ORDER BY REVIEW_DATE DESC) N)S
		WHERE S.R BETWEEN #{startRow} AND #{endRow} 
	</select>
	
</mapper>


















