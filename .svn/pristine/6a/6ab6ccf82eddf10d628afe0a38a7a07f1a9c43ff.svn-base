package com.astral.app.review.vo;

public class HeartVO {
	private int heart_no;
	private int review_no;
	private int mem_no;
	private int heart_check;
	
	public HeartVO() {;}

	public int getHeart_no() {
		return heart_no;
	}

	public void setHeart_no(int heart_no) {
		this.heart_no = heart_no;
	}

	public int getReview_no() {
		return review_no;
	}

	public void setReview_no(int review_no) {
		this.review_no = review_no;
	}

	public int getMem_no() {
		return mem_no;
	}

	public void setMem_no(int mem_no) {
		this.mem_no = mem_no;
	}

	public int getHeart_check() {
		return heart_check;
	}

	public void setHeart_check(int heart_check) {
		this.heart_check = heart_check;
	}
}
