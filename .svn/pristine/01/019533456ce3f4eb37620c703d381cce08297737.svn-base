CREATE TABLE TBL_FOOD(
   FOOD_NAME VARCHAR2(20),
   TIME_NO NUMBER(10),
   PPL_COUNT NUMBER(2),
   PRICE NUMBER(10),
   STARV_LEVEL NUMBER(2),
   CONSTRAINT FOOD_PK PRIMARY KEY(FOOD_NAME)
);

/* 
점심, 저녁 - 3
아침, 점심 - 2
아침 - 1
상관없음 - 0

인원수 
