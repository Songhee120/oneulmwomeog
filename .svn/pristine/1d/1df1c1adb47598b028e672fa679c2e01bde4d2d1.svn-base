<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http//mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="food">
	<select id="FoodList" parameterType="map" resultType="string">
		SELECT FOOD_NAME FROM TBL_FOOD WHERE PPL_COUNT = #{ppl_count} AND PRICE= #{price} AND STARV_LEVEL = #{starv_level}
	</select>
</mapper>