package com.astral.app.food.vo;

public class FoodVO {
	private String food_name;
	private int time_no;
	private int ppl_count;
	private int price;
	private int starv_level;
	
	public FoodVO() {;}

	public String getFood_name() {
		return food_name;
	}

	public void setFood_name(String food_name) {
		this.food_name = food_name;
	}

	public int getTime_no() {
		return time_no;
	}

	public void setTime_no(int time_no) {
		this.time_no = time_no;
	}

	public int getPpl_count() {
		return ppl_count;
	}

	public void setPpl_count(int ppl_count) {
		this.ppl_count = ppl_count;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getStarv_level() {
		return starv_level;
	}

	public void setStarv_level(int starv_level) {
		this.starv_level = starv_level;
	}	
}
